<?xml version="1.0" encoding="utf-8"?>
<ShellsConfiguration
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://schemas.qualisystems.com/ResourceManagement/ShellsConfigurationSchema.xsd">
    <ResourceTemplates>
        <ResourceTemplate Name="Kubernetes" Model="Kubernetes" Driver="K8 Shell Driver" AddressRequired="false">
            <Description>asd.</Description>
            <AutoLoad Enable="true">
                <Description>After filling in all the inputs click Continue to complete the cloud provider creation process.</Description>
            </AutoLoad>
            <Attributes>
                <Attribute Value="" Name="Service Account Username"/>
                <Attribute Value="" Name="CA Cert"/>
                <Attribute Value="" Name="Private Access Key"/>
                <Attribute Value="" Name="IP Address"/>
                <Attribute Value="" Name="Port"/>
                <Attribute Value="" Name="Execution Server Selector"/>
            </Attributes>
        </ResourceTemplate>
    </ResourceTemplates>
</ShellsConfiguration>
